{
  "name": "heroku-deploy",
  "version": "4.0.0",
  "description": "A simple github action that dynamically deploys an app to heroku",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -m --license licenses.txt",
    "test": "node scripts/test",
    "test-action": "node scripts/test-action",
    "setup": "node scripts/setup",
    "start": "npm run build && node dist/index",
    "add-step": "node scripts/add-step",
    "local-test": "act --secret-file my.secrets -P ubuntu-latest=nektos/act-environments-ubuntu:18.04 -j local-deploy-test-1 -v -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkhileshNS/heroku-deploy.git"
  },
  "keywords": [
    "heroku",
    "github",
    "github-actions"
  ],
  "author": "AkhileshNS",
  "bugs": {
    "url": "https://github.com/AkhileshNS/heroku-deploy/issues"
  },
  "homepage": "https://github.com/AkhileshNS/heroku-deploy#readme",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "phin": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@vercel/ncc": "^0.28.6",
    "js-yaml": "^4.1.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  }
}
